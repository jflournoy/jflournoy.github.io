@article{mattaMakingUnknownUnknown2018,
  title = {Making an Unknown Unknown a Known Unknown: {{Missing}} Data in Longitudinal Neuroimaging Studies},
  shorttitle = {Making an Unknown Unknown a Known Unknown},
  author = {Matta, Tyler H. and Flournoy, John C. and Byrne, Michelle L.},
  date = {2018-10-01},
  journaltitle = {Developmental Cognitive Neuroscience},
  shortjournal = {Developmental Cognitive Neuroscience},

@article{rodmanYearSocialLife2021,
  ids = {rodman2021a},
  title = {A {{Year}} in the {{Social Life}} of a {{Teenager}}: {{Within-Persons Fluctuations}} in {{Stress}}, {{Phone Communication}}, and {{Anxiety}} and {{Depression}}},
  shorttitle = {A {{Year}} in the {{Social Life}} of a {{Teenager}}},
  author = {Rodman, Alexandra M. and Vidal Bustamante, Constanza M. and Dennison, Meg J. and Flournoy, John C. and Coppersmith, Daniel D. L. and Nook, Erik C. and Worthington, Steven and Mair, Patrick and McLaughlin, Katie A.},
  date = {2021-04-26},
  journaltitle = {Clinical Psychological Science},
  shortjournal = {Clinical Psychological Science},
  pages = {2167702621991804},
  publisher = {SAGE Publications Inc},
  issn = {2167-7026},
  doi = {10.1177/2167702621991804},
  url = {https://doi.org/10.1177/2167702621991804},
  urldate = {2021-05-20},
  abstract = {Stressful life events (SLEs) are strongly associated with the emergence of adolescent anxiety and depression, but the underlying mechanisms remain poorly understood, especially at the within-persons level. We investigated how adolescent social communication (i.e., frequency of calls and texts) following SLEs relates to changes in internalizing symptoms in a multitimescale, intensive, year-long study (N = 30; n = 355 monthly observations; n ≈ 5,000 experience-sampling observations). Within-persons increases in SLEs were associated with receiving more calls than usual at both the month and moment levels and making more calls at the month level. Increased calls were prospectively associated with worsening internalizing symptoms at the month level only, suggesting that SLEs rapidly influence phone communication patterns, but these communication changes may have a more protracted, cumulative influence on internalizing symptoms. Finally, increased incoming calls prospectively mediated the association between SLEs and anxiety at the month level. We identify adolescent social communication fluctuations as a potential mechanism conferring risk for stress-related internalizing psychopathology.},
  langid = {english},
  keywords = {anxiety,Clinical Psychology,depression,Developmental Psychology,longitudinal,open data,open materials,phone communication,Social and Behavioral Sciences,Social and Personality Psychology,stress},
  file = {/Users/john-flournoy/Zotero/storage/GTXF36W3/Rodman_et_al-2021-A_Year_in_the_Social_Life_of_a_Teenager.pdf;/Users/john-flournoy/Zotero/storage/M3UAF5X4/Rodman_et_al-2020-A_year_in_the_social_life_of_a_teenager.pdf;/Users/john-flournoy/Zotero/storage/PP6UQCEL/aekjt.html}
}

@article{romeoLanguageDevelopmentMechanism2022,
  title = {Language Development as a Mechanism Linking Socioeconomic Status to Executive Functioning Development in Preschool},
  author = {Romeo, Rachel R. and Flournoy, John C. and McLaughlin, Katie A. and Lengua, Liliana J.},
  date = {2022},
  journaltitle = {Developmental Science},
  volume = {25},
  number = {5},
  pages = {e13227},
  issn = {1467-7687},
  doi = {10.1111/desc.13227},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/desc.13227},
  urldate = {2023-04-03},
  abstract = {Childhood socioeconomic status (SES) is related to disparities in the development of both language and executive functioning (EF) skills. Emerging evidence suggests that language development may precede and provide necessary scaffolding for EF development in early childhood. The present preregistered study investigates how these skills co-develop longitudinally in early childhood and whether language development explains the relationship between SES and EF development. A socioeconomically diverse sample of 305 children completed repeated assessments of language (sentence comprehension) and EF (cognitive flexibility, behavioral inhibition, and cognitive inhibition) at four waves spaced 9 months apart from ages 3 to 5 years. Bivariate latent curve models with structured residuals were estimated to disaggregate between-person and within-person components of stability and change. Results revealed bidirectional relationships between language and EF across all waves. However, at 3 years, language comprehension more strongly predicted EF than the reverse; yet by 5 years, the bidirectional effects across domains did not significantly differ. Children from higher-SES backgrounds exhibited higher initial language and EF skills than children from lower-SES families, though SES was not associated with either rate of growth. Finally, early language-mediated the association between SES and early EF skills, and this model outperformed a reverse direction mediation. Together, results suggest that EF development is driven by early language development, and that SES disparities in EF are explained, at least in part, by early differences in language comprehension. These findings have implications for early interventions to support children's language skills as a potential pathway to improving early EF development.},
  langid = {english},
  keywords = {cognitive development,executive functioning,language,longitudinal modeling,socioeconomic status},
  file = {/Users/john-flournoy/Zotero/storage/WPMB8ADG/Romeo et al. - 2022 - Language development as a mechanism linking socioe.pdf;/Users/john-flournoy/Zotero/storage/MUKPAZBC/desc.html}
}


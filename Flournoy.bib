}

@article{mclaughlinChallengesLatentVariable2023,
  title = {Challenges with {{Latent Variable Approaches}} to {{Operationalizing Dimensions}} of {{Childhood}} Adversity – a {{Commentary}} on {{Sisitsky}} et al. (2023)},

@article{flanneryNeurodevelopmentalChangesAdolescence2017,
  title = {Neurodevelopmental Changes across Adolescence in Viewing and Labeling Dynamic Peer Emotions},
  author = {Flannery, Jessica E. and Giuliani, Nicole R. and Flournoy, John C. and Pfeifer, Jennifer H.},
  date = {2017-06-01},
